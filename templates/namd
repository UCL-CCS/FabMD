cd $job_results
$run_prefix

/usr/bin/env > env.log

echo "Start $namd_exec program..."
start_time="$(date -u +%s.%N)"

$namd_exec $namd_input

end_time="$(date -u +%s.%N)"
echo "Done ..."

elapsed="$(bc <<<"$end_time-$start_time")"
echo -e "\n\nTotal Executing Time = $elapsed seconds\n" > time.txt

